# Copy to .env and fill in your values
PORT=8787
OPENAI_API_KEY=sk-your-key-here

# Audio API models (adjust as needed)
OPENAI_AUDIO_TRANSCRIBE_MODEL=whisper-1
OPENAI_AUDIO_TRANSLATE_MODEL=whisper-1

# Optional text model for segment translation (used when targetLanguage is provided)
OPENAI_TEXT_TRANSLATE_MODEL=gpt-4o-mini
ENABLE_TEXT_SEGMENT_TRANSLATION=false
